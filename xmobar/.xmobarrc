Config { font = "xft:Noto Sans CJK SC-12,FuraCode Nerd Font-12"
       , border = BottomB
       , bgColor = "#28342c"
       , fgColor = "#abb2bf"
       , borderColor  = "black"
       , position = Top
       , alpha = 230
       , overrideRedirect = False
       , commands = [
              Run DynNetwork [] 10
            , Run Cpu ["-L","3","-H","50","--normal","green","--high","red"] 10
            , Run Memory ["-t" ,"RAM: <usedratio>%"] 10
            , Run Battery [ "--template" , "Batt: <acstatus>"
              , "--Low"      , "10"        -- units: %
              , "--High"     , "80"        -- units: %
              , "--low"      , "red"
              , "--normal"   , "blue"
              , "--high"     , "green"
              , "--"
              , "-o"	, "<left>% (<timeleft>)"
              , "-O"	, "<fn=1>\61926</fn>"
              , "-i"	, "<fn=1>\62016</fn>"
            ] 100
            , Run Date "%d %b %T" "date" 10
            , Run UnsafeXMonadLog
            ]
, template = "%UnsafeXMonadLog% } %cpu% | %memory% | %dynnetwork% { %battery% | %date% |"
}

